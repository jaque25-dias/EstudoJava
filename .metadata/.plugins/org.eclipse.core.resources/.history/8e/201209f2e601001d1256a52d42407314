public class TestaBanco {

	public static void main(String[] args) {
		Cliente Jaqueline = new Cliente();
		Jaqueline.nome = "Jaqueline";
		Jaqueline.cpf = "000.111.222-33";
		Jaqueline.profissao = "Programadora";

		Conta contaDeJack = new Conta();
		contaDeJack.deposita(1000);
		contaDeJack.saldo = (100);

		contaDeJack.titular = Jaqueline;
		System.out.println("Olá " + contaDeJack.titular.nome);
		System.out.println(" O saldo da sua conta é " + contaDeJack.saldo);
		
	}
}