
public class Peixe extends Animal {

	int nPatas=0;
	String habitat= "mar";
	String cor= "cinza";
	String caracteristicas;
	
	

	public Peixe(String nome, int comprimento, int nPatas, String cor, String habitat, double velocidade, int nPatas2,
			String habitat2, String cor2, String caracteristicas) {
		super(nome, comprimento, nPatas, cor, habitat, velocidade);
		nPatas = nPatas2;
		habitat = habitat2;
		cor = cor2;
		this.caracteristicas = caracteristicas;
	}
	public int getnPatas() {
		return nPatas;
	}
	public void setnPatas(int nPatas) {
		this.nPatas = nPatas;
	}
	public String getHabitat() {
		return habitat;
	}
	public void setHabitat(String habitat) {
		this.habitat = habitat;
	}
	public String getCor() {
		return cor;
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	public String getCaracteristicas() {
		return caracteristicas;
	}
	public void setCaracteristicas(String caracteristicas) {
		this.caracteristicas = caracteristicas;
	}	
	
}
