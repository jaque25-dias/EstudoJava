public class TestaBanco {

	public static void main(String[] args) {
		Cliente Jaqueline = new Cliente();
		Jaqueline.nome = "Jaqueline";
		Jaqueline.cpf = "000.111.222-33";
		Jaqueline.profissao = "Programadora";

		Conta contaDeJack = new Conta();
		contaDeJack.deposita(1000);
		contaDeJack.saldo = (100);
		contaDeJack.agencia = (2034);
		contaDeJack.numero = (4);

		contaDeJack.titular = Jaqueline;
		System.out.println("Olá " + contaDeJack.titular.nome);
		System.out.println(" O CPF é " + contaDeJack.titular.cpf);
		System.out.println("Sua profissão é " + contaDeJack.titular.profissao);
		
		Cliente Ariane = new Cliente();
		Ariane.nome = Ariane;
	    Ariane.cpf = "222.222.222.44";
		Ariane.profissao = "Programadora";
		
		Conta contaDaAriane = new Conta();
		contaDeJack.saldo = (300);
		contaDeJack.agencia = (2031);
		contaDeJack.numero = (0);
				
		contaDaAriane.titular = Ariane;
		System.out.println("Olá " + contaDaAriane.titular.nome);
		System.out.println(" O CPF é " + contaDaAriane.titular.cpf);
		System.out.println("Sua profissão é " + contaDaAriane.titular.profissao);		
		
		}
}