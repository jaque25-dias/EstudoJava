

public abstract class Conta {

    private int cpf;
    protected String titular;
	
	public Conta(int cpf, String titular) {
		this.cpf = cpf;
		this.titular = titular;
					
	}
	public int getCpf() {
		return cpf;
	}
	public void setCpf(int cpf) {
		this.cpf = cpf;
	}
	public String getTitular() {
		return titular;
	}
	public void setTitular(String titular) {
		this.titular = titular;
	}
	
}