	public static sacar(double valor) {
		if (saldo >= valor)
			saldo -= valor;
		saldo++;
		System.out.println("O valor sacaddo foi : " + valor);
		System.out.println("O novo saldo é : " + saldo + "\n");
	}

	if(saque<3){
